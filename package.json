{
  "name": "acav-we-sample-server",
  "version": "1.0.0",
  "author": "aleab (https://github.com/aleab)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleab/acav-we-sample-server.git"
  },
  "license": "MIT",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint -c .eslintrc ./src/**/*.ts",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean && npm run lint",
    "build:be": "tsc --build ./src/backend/tsconfig.json",
    "build:fe": "cross-env NODE_ENV=production webpack -p --mode production",
    "build": "npm run build:be && npm run build:fe",
    "postbuild": "ncp ./src/index.html ./dist/static/index.html",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "open": "^7.0.3",
    "preact": "^10.3.4",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.4",
    "@types/node": "^13.9.8",
    "@types/webpack": "^4.41.10",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dotenv-plugin": "^2.1.0"
  }
}
